            <Font size="20.0" />
         </font>
      </Label>
      <Button fx:id="CoachProfileEditConfirmButton" layoutX="526.0" layoutY="360.0" mnemonicParsing="false" onAction="#OnCoachProfileEditConfirmButtonClick" style="-fx-background-color: white;" text="Confirm" />
      <TextField fx:id="CoachProfileEditAgeTextField" layoutX="283.0" layoutY="201.0" />
      <Label alignment="BOTTOM_LEFT" layoutX="111.0" layoutY="243.0" text="Change Password:" textFill="WHITE">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <PasswordField fx:id="CoachProfileEditChangePasswordPassField" layoutX="283.0" layoutY="243.0" />
   </children>
</AnchorPane>
