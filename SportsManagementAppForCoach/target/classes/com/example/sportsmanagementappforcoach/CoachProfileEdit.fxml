<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.sportsmanagementappforcoach.CoachProfileEdit">
   <children>
      <ImageView fitHeight="400.0" fitWidth="600.0" pickOnBounds="true" smooth="false">
         <image>
            <Image url="@login%202.jpg" />
         </image>
      </ImageView>
      <Label layoutX="198.0" layoutY="200.0" text="Age:" textFill="WHITE">
         <font>
            <Font size="19.0" />
         </font>
      </Label>
      <Label layoutX="186.0" layoutY="291.0" text="Email:" textFill="WHITE">
         <font>
            <Font size="19.0" />
         </font>
      </Label>
      <Label fx:id="CoachProfileEditEmailLabel" layoutX="283.0" layoutY="291.0" prefHeight="24.0" prefWidth="209.0" textFill="WHITE">
         <font>
            <Font size="19.0" />
         </font>
      </Label>
      <Button fx:id="CoachProfileEditButton" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#OnCoachProfileEditButtonClick" style="-fx-background-color: white;" text="Back" />
      <Label fx:id="CoachProfileEditNameLabel" layoutX="198.0" layoutY="52.0" prefHeight="26.0" prefWidth="169.0" textFill="WHITE">
